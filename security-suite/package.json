{
  "name": "solana-security-suite",
  "version": "1.0.0",
  "description": "Comprehensive Solana Smart Contract Security Testing Suite",
  "main": "backend/src/server.ts",
  "scripts": {
    "dev": "tsx watch backend/src/server.ts",
    "start": "tsx backend/src/server.ts",
    "build": "tsc",
    "test:static": "python3 detectors/run_all.py",
    "test:anchor": "tsx backend/src/tests/run-tests.ts",
    "test:fuzz": "cd ../programs/app-market && cargo fuzz run fuzz_target_1 -- -max_total_time=60",
    "test:api": "tsx backend/src/tests/api-tests.ts",
    "test:all": "npm run test:static && npm run test:anchor && npm run test:api",
    "report": "tsx backend/src/results/generate-report.ts"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.29.0",
    "@solana/web3.js": "^1.91.1",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "pdfkit": "^0.14.0",
    "socket.io": "^4.7.4",
    "uuid": "^9.0.1",
    "solana-bankrun": "^0.3.0",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.19",
    "@types/pdfkit": "^0.13.4",
    "@types/uuid": "^9.0.8",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}
